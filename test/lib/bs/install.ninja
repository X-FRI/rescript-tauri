rescript = 1
rule cp
  command = cp $i $out
rule touch
 command = touch $out
o Demo.cmi : cp ../bs/../src/Demo.cmi
o Demo.cmj : cp ../bs/../src/Demo.cmj
o Demo.cmt : cp ../bs/../src/Demo.cmt
o Demo.res : cp ../../../src/Demo.res
o Tauri.cmi : cp ../bs/../src/Tauri.cmi
o Tauri.cmj : cp ../bs/../src/Tauri.cmj
o Tauri.cmt : cp ../bs/../src/Tauri.cmt
o Tauri.res : cp ../../../src/Tauri.res
o App.cmi : cp ../bs/src/App.cmi
o App.cmj : cp ../bs/src/App.cmj
o App.cmt : cp ../bs/src/App.cmt
o App.res : cp ../../src/App.res
o Main.cmi : cp ../bs/src/Main.cmi
o Main.cmj : cp ../bs/src/Main.cmj
o Main.cmt : cp ../bs/src/Main.cmt
o Main.res : cp ../../src/Main.res
build install.stamp : touch Demo.cmi Demo.cmj Tauri.cmi Tauri.cmj App.cmi App.cmj Main.cmi Main.cmj 
